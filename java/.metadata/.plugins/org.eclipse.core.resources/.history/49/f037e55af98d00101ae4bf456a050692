package ch1;

/*
	조건문이란?
	조건이 참이면 실행

	if(조건) {
		실행문
	}
 */

public class Control1 {

	public static void main(String[] args) {

		// if문
		if (true) {
			System.out.println("참입니다");
		}

		// if-else문
		if (10 > 20) {
			System.out.println("10은 20보다 큽니다");
		} else {
			System.out.println("10은 20보다 작습니다");
		}
	}

}
